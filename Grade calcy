#include<stdio.h>
intmain()
{
int marks ;
printf("Enter the marks : %d" , marks);
scanf("%d", &marks);

if(marks>69)
{
if (marks>85){printf("The grade is : A");}
else{printf("The grade is :B");}
}

else if(marks>39)
{
if (marks>55){printf("The grade is : C");}
else{ printf("The grade is : D");}
}

else{
printf("The grade is: F");
}
return 0 ;
}
